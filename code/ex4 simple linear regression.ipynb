   "source": [
    "# IQR function which return all IQR values of data.\n",
    "def calculate_IQR(data):\n",
    "    q1= np.quantile(data,0.25)\n",
    "    q2 = np.median(data)\n",
    "    q3= np.quantile(data,0.75)\n",
    "    iqr = q3-q1\n",
    "    lower_bound=q1-1.5*iqr\n",
    "    upper_bound = q3+1.5*iqr\n",
    "    return {'q1':q1,\n",
    "            'median':q2,\n",
    "            'q3':q3,\n",
    "            'iqr':iqr,\n",
    "            'lower_bound':lower_bound,\n",
    "            'upper_bound':upper_bound}"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "8e368790-6e44-4c2c-80af-5d16fb749e1a",
   "metadata": {},
   "outputs": [],
   "source": [
    "# remove outlier of each column using loop\n",
    "for col in data.drop('gas',axis=1).columns:\n",
    "    iqr_values = calculate_IQR(data[col])\n",
    "    filter_data = data[(data[col]>iqr_values['lower_bound']) & (data[col]<iqr_values['upper_bound'])]\n",
    "    "
   ]
  },
